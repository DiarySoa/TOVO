@model TOVO.Models.Service_Poste

<h1>
    <center>Service</center>
</h1>

<form method="post" action="@Url.Action("ValiderListe1","Service_Poste")">
<p>Services
    <select name="nom_service">
        @foreach (var service in Model.services)
        {
            <option>@service.nom_service</option>
        }
    </select>
</p>

<p>Valeur horaire <input type="text" name="heure"></p>

<p>Postes
    <select name="nom_poste">
        @foreach (var poste in Model.postes)
        {
            <option>@poste.nom_poste</option>
        }
    </select>
</p>

<p>Diplome
    <select name="diplome">
        <option value="CEPE">CEPE</option>
        <option value="BEPC">BEPC</option>
        <option value="BACC">BACC</option>
        <option value="LICENSE">LICENSE</option>
        <option value="MASTER">MASTER</option>
    </select>
</p>
<input type="submit" value="Enregistrer" name="enregistrer">
</form>


